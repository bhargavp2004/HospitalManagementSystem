<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IPatientService" />
                <binding name="BasicHttpBinding_IAdminService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/HMSService/PatientService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPatientService"
                contract="PatientServiceRef.IPatientService" name="BasicHttpBinding_IPatientService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/HMSService/AdminService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAdminService"
                contract="AdminServiceRef.IAdminService" name="BasicHttpBinding_IAdminService" />
        </client>
    </system.serviceModel>
</configuration>